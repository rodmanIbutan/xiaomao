<view class="container order-list-page"><block wx:for="{{orderList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="order-item"><navigator class="order-title line-ellipsis" url="{{'../cinema-detail/cinema-detail?cinemaId='+item.cinemaId}}" hover-class="none"><view>{{item.cinemaName}}</view><view class="triangle"></view></navigator><view class="order-info" data-order="{{item}}"><image src="{{item.movieImg}}"></image><view class="order-desc"><view class="delete-box" data-index="{{index}}" data-event-opts="{{[['tap',[['deleteOrder',['$event']]]]]}}" catchtap="__e"><view class="iconfont icon-del delete"></view></view><view class="snack-name line-ellipsis">{{item.movieName}}</view><view class="amount line-ellipsis">{{"数量："+item.seats}}</view></view></view><view class="order-more"><view>{{"总价："+item.orderPrice+"元"}}</view><view class="status">已完成</view></view></view></block><block wx:if="{{!$root.g0}}"><view><block wx:if="{{false}}"><block name="nothing"><view class="nothing"><image class="nothing-img" src="/static/image/user/nothing.png"></image><view class="nothing-text">没有找到相关内容</view></view></block></block></view></block></view>